{
  "name": "@supertokens-plugins/base-profile-react",
  "version": "0.0.1-beta.1",
  "description": "Base Profile Plugin for SuperTokens",
  "homepage": "https://github.com/supertokens/supertokens-plugins/blob/main/packages/base-profile-react/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supertokens/supertokens-plugins.git",
    "directory": "packages/base-profile-react"
  },
  "scripts": {
    "build": "vite build && npm run pretty",
    "pretty": "npx pretty-quick .",
    "pretty-check": "npx pretty-quick --check ."
  },
  "keywords": [
    "base-profile",
    "plugin",
    "supertokens"
  ],
  "dependencies": {
    "@shared/js": "*",
    "@shared/react": "*",
    "@shared/ui": "*",
    "supertokens-js-override": "^0.0.4"
  },
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1",
    "supertokens-auth-react": ">=0.50.0"
  },
  "devDependencies": {
    "@shared/eslint": "*",
    "@shared/tsconfig": "*",
    "@types/react": "^17.0.20",
    "prettier": "3.5.3",
    "pretty-quick": "^4.2.2",
    "typescript": "^5.8.3"
  },
  "browser": {
    "fs": false
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "dist/index.d.ts",
      "default": "dist/index.js"
    },
    "./index": {
      "types": "dist/index.d.ts",
      "default": "dist/index.js"
    },
    "./index.js": {
      "types": "dist/index.d.ts",
      "default": "dist/index.js"
    }
  }
}
