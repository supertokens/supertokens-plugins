{
  "name": "@supertokens-plugins/captcha-react",
  "version": "0.1.0",
  "description": "SuperTokens plugin that adds CAPTCHA support in your React application.",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "keywords": [],
  "exports": {
    ".": {
      "types": "dist/index.d.ts",
      "default": "dist/index.js"
    },
    "./index": {
      "types": "dist/index.d.ts",
      "default": "dist/index.js"
    },
    "./index.js": {
      "types": "dist/index.d.ts",
      "default": "dist/index.js"
    }
  },
  "license": "Apache-2.0",
  "dependencies": {
    "supertokens-auth-react": "github:supertokens/supertokens-auth-react#feat/plugin/base",
    "@supertokens/auth-ui": "*"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@shared/eslint": "*",
    "@shared/tsconfig": "*",
    "@testing-library/jest-dom": "^6.1.0",
    "@types/cloudflare-turnstile": "^0.2.2",
    "@types/grecaptcha": "^3.0.9",
    "@types/react": "^18.3.1",
    "eslint": "^8.57.0",
    "jsdom": "^26.1.0",
    "prettier": "2.0.5",
    "pretty-quick": "^3.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "vitest": "^1.3.1"
  },
  "browser": {
    "fs": false
  },
  "publishConfig": {
    "access": "public"
  }
}
